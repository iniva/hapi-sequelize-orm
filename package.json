{
  "name": "hapi-sequelize-orm",
  "version": "1.0.2",
  "description": "Sequelize plugin for HapiJS (v17+)",
  "main": "lib/index.js",
  "author": "Tony DBR <tonyblast@gmail.com>",
  "license": "MIT",
  "keywords": [
    "hapijs",
    "sequelize",
    "orm"
  ],
  "files": [
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "clean:build": "rm lib/*.test.js",
    "commit": "git-cz",
    "hooks:message": "node hooks/index.js --action=message",
    "do:build": "yarn clean && babel src --out-dir lib --copy-files --ignore '**/*.test.js' && yarn clean:build",
    "do:publish": "yarn do:build && yarn publish",
    "lint": "eslint src hooks",
    "test": "NODE_ENV=test jest",
    "test:coverage": "yarn test --coverage",
    "test:coveralls": "yarn test --coverage --coverageReporters=text-lcov | coveralls"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@hapi/hapi": "^18.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "commitizen": "^3.1.1",
    "coveralls": "^3.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.16.0",
    "husky": "^2.1.0",
    "jest": "^24.7.1",
    "mysql2": "^1.6.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "sequelize": "^5.7.6"
  }
}
